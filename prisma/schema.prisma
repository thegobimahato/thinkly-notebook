generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               String  @id @unique
  name             String?
  email            String  @unique
  stripeCustomerId String? @unique
  colorScheme      String  @default("theme-blue")
}
